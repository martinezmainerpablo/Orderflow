{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "orderflow": {
      "error": "This resource does not support generation in the manifest."
    },
    "postgres": {
      "type": "container.v1",
      "connectionString": "Host={postgres.bindings.tcp.host};Port={postgres.bindings.tcp.port};Username=postgres;Password={postgres-password.value}",
      "image": "docker.io/library/postgres:17.6",
      "volumes": [
        {
          "name": "orderflow-8bbf972b91-postgres-data",
          "target": "/var/lib/postgresql/data",
          "readOnly": false
        }
      ],
      "env": {
        "POSTGRES_HOST_AUTH_METHOD": "scram-sha-256",
        "POSTGRES_INITDB_ARGS": "--auth-host=scram-sha-256 --auth-local=scram-sha-256",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "{postgres-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5432
        }
      }
    },
    "redis": {
      "type": "container.v1",
      "connectionString": "{redis.bindings.tcp.host}:{redis.bindings.tcp.port},password={redis-password.value}",
      "image": "docker.io/library/redis:8.2",
      "entrypoint": "/bin/sh",
      "args": [
        "-c",
        "redis-server --requirepass $REDIS_PASSWORD --save 60 1"
      ],
      "volumes": [
        {
          "name": "redis-data-identity",
          "target": "/data",
          "readOnly": false
        }
      ],
      "env": {
        "REDIS_PASSWORD": "{redis-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 6379
        }
      }
    },
    "rabbitMQ": {
      "type": "container.v1",
      "connectionString": "amqp://guest:{rabbitMQ-password-uri-encoded.value}@{rabbitMQ.bindings.tcp.host}:{rabbitMQ.bindings.tcp.port}",
      "image": "docker.io/library/rabbitmq:4.2-management",
      "volumes": [
        {
          "name": "rabbitMQ",
          "target": "/var/lib/rabbitmq",
          "readOnly": false
        }
      ],
      "env": {
        "RABBITMQ_DEFAULT_USER": "guest",
        "RABBITMQ_DEFAULT_PASS": "{rabbitMQ-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5672
        },
        "management": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 15672
        }
      }
    },
    "maildev": {
      "type": "container.v1",
      "image": "maildev/maildev:latest",
      "bindings": {
        "web": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 1080
        },
        "smtp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 1025
        }
      }
    },
    "identitydb": {
      "type": "value.v0",
      "connectionString": "{postgres.connectionString};Database=identitydb"
    },
    "categorydb": {
      "type": "value.v0",
      "connectionString": "{postgres.connectionString};Database=categorydb"
    },
    "ordersdb": {
      "type": "value.v0",
      "connectionString": "{postgres.connectionString};Database=ordersdb"
    },
    "orderflow-identity": {
      "type": "project.v1",
      "path": "../Orderflow.Identity/Orderflow.Identity.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{orderflow-identity.bindings.http.targetPort}",
        "ConnectionStrings__identitydb": "{identitydb.connectionString}",
        "ConnectionStrings__rabbitMQ": "{rabbitMQ.connectionString}",
        "RABBITMQ_HOST": "{rabbitMQ.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitMQ.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitMQ-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitMQ-password-uri-encoded.value}@{rabbitMQ.bindings.tcp.host}:{rabbitMQ.bindings.tcp.port}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflow-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "orderflow-identity"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "orderflow-notification": {
      "type": "project.v1",
      "path": "../Orderflow.Notification/Orderflow.Notification.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "Email__SmtpHost": "{maildev.bindings.smtp.host}",
        "Email__SmtpPort": "{maildev.bindings.smtp.port}",
        "ConnectionStrings__rabbitMQ": "{rabbitMQ.connectionString}",
        "RABBITMQ_HOST": "{rabbitMQ.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitMQ.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitMQ-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitMQ-password-uri-encoded.value}@{rabbitMQ.bindings.tcp.host}:{rabbitMQ.bindings.tcp.port}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflow-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "orderflow-notification"
      }
    },
    "orderflow-catalog": {
      "type": "project.v1",
      "path": "../Orderflow.Catalog/Orderflow.Catalog.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{orderflow-catalog.bindings.http.targetPort}",
        "ConnectionStrings__categorydb": "{categorydb.connectionString}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflow-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "orderflow-catalog"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "orderflow-orders": {
      "type": "project.v1",
      "path": "../Orderflow.Orders/Orderflow.Orders.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{orderflow-orders.bindings.http.targetPort}",
        "ConnectionStrings__ordersdb": "{ordersdb.connectionString}",
        "ConnectionStrings__rabbitMQ": "{rabbitMQ.connectionString}",
        "RABBITMQ_HOST": "{rabbitMQ.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitMQ.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitMQ-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitMQ-password-uri-encoded.value}@{rabbitMQ.bindings.tcp.host}:{rabbitMQ.bindings.tcp.port}",
        "ORDERFLOW-CATALOG_HTTP": "{orderflow-catalog.bindings.http.url}",
        "services__orderflow-catalog__http__0": "{orderflow-catalog.bindings.http.url}",
        "ORDERFLOW-CATALOG_HTTPS": "{orderflow-catalog.bindings.https.url}",
        "services__orderflow-catalog__https__0": "{orderflow-catalog.bindings.https.url}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflow-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "orderflow-orders"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "orderflow-apigateway": {
      "type": "project.v1",
      "path": "../Orderflow.ApiGateway/Orderflow.ApiGateway.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{orderflow-apigateway.bindings.http.targetPort}",
        "ConnectionStrings__redis": "{redis.connectionString}",
        "REDIS_HOST": "{redis.bindings.tcp.host}",
        "REDIS_PORT": "{redis.bindings.tcp.port}",
        "REDIS_PASSWORD": "{redis-password.value}",
        "REDIS_URI": "redis://:{redis-password-uri-encoded.value}@{redis.bindings.tcp.host}:{redis.bindings.tcp.port}",
        "ConnectionStrings__rabbitMQ": "{rabbitMQ.connectionString}",
        "RABBITMQ_HOST": "{rabbitMQ.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitMQ.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitMQ-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitMQ-password-uri-encoded.value}@{rabbitMQ.bindings.tcp.host}:{rabbitMQ.bindings.tcp.port}",
        "ORDERFLOW-IDENTITY_HTTP": "{orderflow-identity.bindings.http.url}",
        "services__orderflow-identity__http__0": "{orderflow-identity.bindings.http.url}",
        "ORDERFLOW-IDENTITY_HTTPS": "{orderflow-identity.bindings.https.url}",
        "services__orderflow-identity__https__0": "{orderflow-identity.bindings.https.url}",
        "ORDERFLOW-CATALOG_HTTP": "{orderflow-catalog.bindings.http.url}",
        "services__orderflow-catalog__http__0": "{orderflow-catalog.bindings.http.url}",
        "ORDERFLOW-CATALOG_HTTPS": "{orderflow-catalog.bindings.https.url}",
        "services__orderflow-catalog__https__0": "{orderflow-catalog.bindings.https.url}",
        "ORDERFLOW-ORDERS_HTTP": "{orderflow-orders.bindings.http.url}",
        "services__orderflow-orders__http__0": "{orderflow-orders.bindings.http.url}",
        "ORDERFLOW-ORDERS_HTTPS": "{orderflow-orders.bindings.https.url}",
        "services__orderflow-orders__https__0": "{orderflow-orders.bindings.https.url}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflow-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "orderflow-apigateway"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "orderflowweb": {
      "type": "container.v1",
      "build": {
        "context": "../orderflow.web",
        "dockerfile": "../orderflow.web/Dockerfile"
      },
      "env": {
        "NODE_ENV": "development",
        "ORDERFLOW-APIGATEWAY_HTTP": "{orderflow-apigateway.bindings.http.url}",
        "services__orderflow-apigateway__http__0": "{orderflow-apigateway.bindings.http.url}",
        "ORDERFLOW-APIGATEWAY_HTTPS": "{orderflow-apigateway.bindings.https.url}",
        "services__orderflow-apigateway__https__0": "{orderflow-apigateway.bindings.https.url}",
        "ORDERFLOW-IDENTITY_HTTP": "{orderflow-identity.bindings.http.url}",
        "services__orderflow-identity__http__0": "{orderflow-identity.bindings.http.url}",
        "ORDERFLOW-IDENTITY_HTTPS": "{orderflow-identity.bindings.https.url}",
        "services__orderflow-identity__https__0": "{orderflow-identity.bindings.https.url}",
        "PORT": "{orderflowweb.bindings.http.targetPort}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflow-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "orderflowweb"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 7000,
          "external": true
        }
      }
    },
    "postgres-password": {
      "type": "parameter.v0",
      "value": "{postgres-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22
            }
          }
        }
      }
    },
    "redis-password": {
      "type": "parameter.v0",
      "value": "{redis-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22,
              "special": false
            }
          }
        }
      }
    },
    "redis-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{redis-password.value}",
      "filter": "uri"
    },
    "rabbitMQ-password": {
      "type": "parameter.v0",
      "value": "{rabbitMQ-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22,
              "special": false
            }
          }
        }
      }
    },
    "rabbitMQ-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{rabbitMQ-password.value}",
      "filter": "uri"
    },
    "orderflow-dashboard": {
      "type": "container.v1",
      "image": "mcr.microsoft.com/dotnet/nightly/aspire-dashboard:latest",
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18888
        },
        "otlp-grpc": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18889
        }
      }
    }
  }
}